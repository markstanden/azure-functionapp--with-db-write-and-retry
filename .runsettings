<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <Format>cobertura</Format>
          <Exclude>[*]Microsoft.Azure.Functions.Worker.*,[*]AddShipmentNotification.Program,[*]WorkerExtensionStartupCodeExecutor,[*]DirectFunctionExecutor,[*]FunctionExecutorHostBuilderExtension,[*]FunctionExecutorAutoStartup,[*]GeneratedFunctionMetadataProvider,[*]WorkerHostBuilderFunctionMetadataProvider,[*]FunctionMetadataProviderAutoStartup</Exclude>
          <ExcludeByAttribute>GeneratedCodeAttribute,CompilerGeneratedAttribute</ExcludeByAttribute>
          <SingleHit>false</SingleHit>
          <UseSourceLink>true</UseSourceLink>
          <IncludeTestAssembly>false</IncludeTestAssembly>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>